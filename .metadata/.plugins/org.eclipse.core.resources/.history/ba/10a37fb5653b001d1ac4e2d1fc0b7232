import java.util.*;
class demo
{
	private int age=30;
	private String  name="leo";
	demo(int a, String b)
	{
		age=a;
		name="gulf";
	}
	
	demo()
	{
	  age=45;
	  name="rolando";
	}
	void result()
	{
		System.out.println("hai "+name);
		System.out.println("your age is "+age);
	}
}
public class Sample8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		demo d1=new demo();
		d1.result();
		demo d2=new demo(23,"peter");
		d2.result();
		
		
		
		
		  
		

	}

}
